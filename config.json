    "options": {
        "email": "",
        "httpreq": {
            "url": "",
            "username": "",
            "password": ""
        }
    },
    "ingress": true,
    "panel_icon":"mdi:routes",
    "schema": {
        "email": "email",
        "httpreq": {
            "url": "str?",
            "username": "str?",
            "password": "str?"
        }
    }
